{
  "manifest_version": 2,

  "name": "FCC reporter",
  "description": "This extension helps you to report videos without subtitles to the FCC",
  "version": "1.0",
  "background": {
       "scripts": ["background.js"],
       "persistent": false
     },
  "permissions":
    ["tabs"]
  ,
  "page_action": {
  "default_icon": "icon.png",
  "default_popup": "popup.html",
  "default_title": "Report page"
  }
  // "permissions": [
  //   "activeTab",
  //   "https://ajax.googleapis.com/"
  // ]
}